---
layout: null
---
{% assign contents_of_page = site.pages | where: "path", "daily.html" | first %}
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>Daily Rundown</title>
    <link>{{ site.url }}/daily</link>
    <description>{{ site.description }}</description>
    <item>
      <title>Daily Rundown for {{ site.date | date: "%Y-%m-%d" }}</title>
      <link>{{ site.url }}/daily</link>
      <description>
        {{ contents_of_page.content }}
      </description>
      <pubDate>{{ site.date | date: "%Y-%m-%d" }}</pubDate>
    </item>
  </channel>
</rss>
